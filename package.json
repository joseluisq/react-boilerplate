{
  "name": "webpack-boilerplate",
  "private": true,
  "description": "Webpack Boilerplate.",
  "repository": "joseluisq/webpack-boilerplate",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "engines": {
    "node": ">= 4.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": ">=2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "4.5.0",
    "postcss-loader": "^1.2.2",
    "readline-sync": "^1.4.6",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config ./config/webpack.config.dev.js",
    "build": "npm run clean && npm run prod",
    "clean": "rm -rf ./dist",
    "prod": "webpack -p --bail --colors --progress --profile --config ./config/webpack.config.prod.js",
    "release": "node ./scripts/release.js",
    "test": "npm run lint && npm run tape",
    "lint": "eslint src"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "dev-expression"
    ]
  }
}
